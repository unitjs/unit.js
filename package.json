    "url": "git://github.com/unitjs/unit.js.git"
  },
  "bugs": "https://github.com/unitjs/unit.js/issues",
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "bluebird": "^2.3.11",
